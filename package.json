{
  "name": "redumd",
  "version": "1.7.0",
  "description": "redux framework",
  "homepage": "https://github.com/zhbhun/redumd",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhbhun/redumd"
  },
  "bugs": {
    "url": "https://github.com/zhbhun/redumd/issues"
  },
  "author": "zhbhun",
  "contributors": [
    {
      "name": "zhbhun",
      "url": "https://github.com/zhbhun"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib",
    "saga"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && babel src --out-dir lib",
    "lint": "eslint ./src",
    "format": "eslint --fix ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "redux": "^4.0.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "lodash.mergewith": "^4.0.0",
    "normalizr": "^3.0.0",
    "redux-devtools-extension": "^2.0.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "eslint-config-ease-base": "^2.0.0",
    "lint-staged": "^7.3.0",
    "prettier-config-ease": "^2.0.0",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "iOS >= 7",
              "Android >= 4.2",
              "last 2 QQAndroid versions"
            ]
          },
          "spec": false,
          "loose": false,
          "modules": "commonjs",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        }
      ]
    ],
    "sourceType": "unambiguous"
  }
}
